{
  "name": "node-consoleapp-template",
  "version": "0.0.1",
  "description": "a basic node template for console apps",
  "main": "console.js",
  "scripts": {
    "console": "better-npm-run start-console",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "start-console": {
      "command": "node ./bin/console.js",
      "env": {
        "NODE_ENV": "production",
        "APPNAME": "console-app"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogovski/node-consoleapp-template.git"
  },
  "keywords": [
    "node",
    "starter",
    "consoleapp",
    "babel"
  ],
  "author": "rogovski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rogovski/node-consoleapp-template/issues"
  },
  "homepage": "https://github.com/rogovski/node-consoleapp-template#readme",
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19"
  },
  "devDependencies": {
    "better-npm-run": "0.0.11",
